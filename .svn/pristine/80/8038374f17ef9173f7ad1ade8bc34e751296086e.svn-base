.productShade{
	height: 100%;
	width: 100%;
	position: absolute;
	z-index: 1000;
	display: none;
	opacity: 0.6;
    filter: alpha(opacity=60);
	background-color: #333333;
}
.productShare
{
	height: 7rem;
	width: 100%;
	position: absolute;
	bottom:  0;
	left: 0;
	background-color: #FFF;
	z-index: 1001;
	display: none;
	.share_top
	{
		padding: 0.5rem 0 0.5rem 0;
		overflow-y: auto;
		background-color: #eee;
		.share_con
		{
			float: left;
			width: 33.3%;
			text-align: center;
			font-size: 14px;
			p{
				margin: 0;
				color: #8e8e8e;
			}
		}
	}
	.share_bottom{background-color: #FFF;height: 42px; border-top: 1px solid #e3e3e3;text-align: center;line-height: 42px;font-size: 16px; color: #333333;}
}

/*produtct not found style*/
#productNotFound{
	background: #fff;
	position: relative;
	.con{
		margin-top: 50px;
	}
	.tips{
		/*padding-top: 20px;*/
		position: absolute;
		top: 45%;
		left: 0;
		width: 100%;
		transform: translateY(-50%);
		text-align: center;
		.errorImg {
			display: inline-block;
			width: 80%;
			img {
				width: 100%;
			}
		}
		.knIndex {
			display: inline-block;
			width: 60%;
			height: 50px;
			margin-top: 50px;
			border: 1px solid #949494;
			border-radius: 45px;
			line-height: 50px;
			text-align: center;
			color: #9b9b9b;
		}
	}
	.recommend-pro{
		padding: 20px 0;
		h2{
			display: flex;
			&:before{
				content:'';
				display: block;
				border-top: 1px solid  #e3e3e3;
				flex:1 1 auto;
				margin-top: 10px;
			}
			&:after{
				content:'';
				display: block;
				border-top: 1px solid  #e3e3e3;
				flex:1 1 auto;
				margin-top: 10px;
			}
			span{
				display: block;
				line-height: 20px;
				font-size: 14px;
				flex:0 1 70px;
				text-align: center;
			}
		}
		.item{
			width: 50%;
			padding: 0 0px 10px 10px;
			/*float: left;*/
			font-size: 12px;
			display: inline-block;
			.item-link{
				display: block;
				overflow: hidden;
			}
			img{
				width: 100%;
				height: auto;
			}
			h4{
				font-size: 14px;
				line-height: 18px;
				max-height: 36px;
				min-height: 36px;
				overflow: hidden;
				color: #333;
				text-overflow: ellipsis;
				overflow: hidden;
				display: -webki-box;
				-webkit-box-orient:vertical;
				-webkit-line-clap:2;
				a{
					color: #333;
				}
			}
			p{
				font-size: 14px;
				color: #e65055;
			}
		}
	}
}





