}
.slide-bar{
		position: fixed;
		right: 20px;
		top: 50%;
		transform:translateY(-50%);
		a{
			display: block;
			width: 30px;
			height: 30px;
			line-height: 30px;
			text-align: center;
			background: #fff;
			border: 1px solid #0cb9f3;
			margin-bottom: 15px;
			border-radius: 30px;
			font-size: 14px;
			color: #0cb9f3
		}
		.cur{
			background-color: #0cb9f3;
			color: #fff;
		}
		.hide{
			display:none;
		}
		.show{
			display:block;
		}
		.prev,.next{
			span{
				font-weight: bold;
				position: relative;
				top: -2px;
			}
		}
		.disabled{
			background: #8e8e8e;
			color: #fff;
			border-color: #8e8e8e;
		}
	}
