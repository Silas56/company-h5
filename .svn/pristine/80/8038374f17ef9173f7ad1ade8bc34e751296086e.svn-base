@import '../../css/common/reset.scss';
@import '../../css/common/style.scss';
//Ïà¶ÔÓÚÉè¼Æ¸åËõ·Å±ÈÀý
$x :1.5;
$backgroundBaseDir : '../../images/detail/';
$blue: #0cb9f3;
a{
	color: #333333;
}
//css sprite class
.ic-share:after,
.ic-place:before,
.ic-destination:before,
.ic-time:before,
.ic-address:before,
.ic-star:before,
.ic-phone:before,
.ic-service:before,
.ic-home:before,
.ic-equipment:before,
.nav-bar .nav-con>a:before{
	background-image: url(#{$backgroundBaseDir}icons-detail.png);
	background-size: 160px/$x 191px/$x;
}

.ic-star:before,
.ic-phone:before,
.ic-service:before,
.ic-home:before,
.ic-equipment:before,
.ic-place:before,
.ic-destination:before,
.ic-time:before,
.ic-address:before{
	content: '';
	display: inline-block;
	width: 24px;
	height: 24px;
	background-position: -25px 0;
	position: relative;
	top: 3px;
	vertical-align: middle;
	margin-right: 5px;
	position: absolute;
	top: 8px;
	left: 0;
}
.ic-star:before{
	background-position: -53px 0;
}
.ic-phone:before{
	background-position: -53px -40px;
}
.ic-service:before{
	background-position: -54px -71px;
}
.ic-home:before{
	background-position: -79px 0px;
}
.ic-equipment:before{
	background-position: -79px -37px;
}
.ic-destination:before{
	background-position: -28px -36px;
}
.ic-time:before{
	background-position: -28px -74px;
}
.ic-address:before{
	top: 3px;
	background-position: -28px -104px;
}
.ic-pro-desc{
	line-height: 22px;
	padding: 0;
}

#productDetail, #snapshotDetail {
	background-color: #f3f3f3;
	font-family: 'Microsoft YaHei';
	.content{
		padding-bottom: 53px;
	}
	a{
		color: #333333;
	}
	.swiper-container{
		padding-bottom: 0;
		.swiper-pagination{
			bottom: 0;
		}
		.swiper-pagination-bullet{
			background-color: #fff;
			opacity: 1;
		}
		.swiper-pagination-bullet-active{
			background-color: #0cb9f2;
		}
	}
	.swiper-wrapper{
		img{
			width: 100%;
			height: auto;
		}
	}
	.info{
		background: #fff;
		border-bottom: 1px solid  #e3e3e3;
		margin-bottom: 26px/$x;
		h1{
			padding: 10px 5px;
			line-height: 24px;
			a{
				color: #333333;
				font-size: 16px;
			}
		}
		.price-wrapper{
			position: relative;
			padding: 0.5rem 0;
			.priceWrapper{
				position:relative;
				width:100%;
				padding-right: 80px;
				word-break: break-all;
				white-space: normal;
			}
		}
		.price{
			padding: 0 0.1rem;
			color: #e65055;
			font-size: 18px;
			font-weight: bold;
			span{
				font-size: 14px;
			}
		}
		del{
			font-size: 12px;
			padding: 0 0.2rem;
		}
		.share{
			position: absolute;
			top: 50%;
			right: 5px;
			transform: translateY(-50%);
			font-size: 14px;
			line-height: 36px/$x;
			&:after{
				content:'';
				display: inline-block;
				width: 36px/$x;
				height: 36px/$x;
				vertical-align: middle;
				margin-left: 4px;
				background-position: 0 0;
			}
		}
	}
	.list-function{
		&>a{
			display: block;
			height: 92px/$x;
			line-height: 92px/$x;
			font-size: 16px;
			padding: 0 5px;
			background: #fff;
			border: 1px solid #e3e3e3;
			margin-bottom: 26px/$x;
			position: relative;
			&:after{
				content:'';
				display: inline-block;
				width: 16px;
				height: 16px;
				border-top: 1px solid  #b2b1b1;
				border-right: 1px solid  #b2b1b1;
				position: absolute;
				top: 22px;
				right: 5px;
				transform:rotate(45deg);				
			}
		}
		h6{
			height: 92px/$x;
			line-height: 92px/$x;
			font-size: 16px;
			border-bottom: 1px solid  #e3e3e3;
		}
		.cost-desc{
			font-size: 14px;
			background: #fff;
			border: 1px solid #e3e3e3;
			margin-bottom: 26px/$x;
			padding: 0 5px;
			p{
				padding: 10px 0;
				line-height: 18px;
				color: #666666;
			}
			img {
				max-width: 100%;
			}
		}
		.product{
			background: #fff;
			border: 1px solid #e3e3e3;
			margin-bottom: 26px/$x;
			padding: 0 5px;
			.p-list>li{
				line-height: 55px/$x;
				font-size: 14px;
				position: relative;
				/*padding-left: 25px;*/
				line-height: 24px;
				padding: 6px 6px 6px 25px;
				color: #666666;
			}
			.p-list .pro-desc{
				padding: 0;
				.cover{
					max-width:100%;
				}
				img{
					max-width:100%;
				}
				p {
					margin-top: 10px;
				}
			}
			.bundleDiv{
				width:33%;
				float:left;
				img{
					width:33%;
				}
			}
			.bundleWord{
				width:66%;
				overflow:hidden;
				font-size: 0.8rem;
				word-wrap: break-word;
			}
		}
	}
	.noComment{
		text-align: center;
		padding: 10px;
		font-size: 14px;
	}
	.comment{
		background: #fff;
		border: 1px solid #e3e3e3;
		margin-bottom: 26px/$x;
		padding: 0 5px 10px 5px;
		h5{
			height: 92px/$x;
			line-height: 92px/$x;
			border-bottom: 1px solid  #e3e3e3;
			font-size: 14px;
			a{
				display: block;
				position: relative;
			}
			a:after{
				content:'';
				display: inline-block;
				width: 16px;
				height: 16px;
				border-top: 1px solid  #b2b1b1;
				border-right: 1px solid  #b2b1b1;
				position: absolute;
				top: 22px;
				right: 5px;
				transform:rotate(45deg);	
			}
			span{
				font-size: 16px;
			}
			em{
				color: #16b8eb;
			}
		}
	}
	.recommend{
		background: #fff;
		border: 1px solid #e3e3e3;
		margin-bottom: 26px/$x;
		padding: 0 5px 10px 5px;
		overflow: hidden;
		h5{
			height: 80px/$x;
			line-height: 92px/$x;
			font-size: 16px;
		}
		.re-con{
			li{
				display: block;
				float: left;
				width: 50%;
				box-sizing:border-box;
				padding: 0 5px 5px 5px;
				img{
					width: 100%;
					height: auto;
				}
				h6{
					font-size: 14px;
					line-height: 20px;
					height: 40px;
					overflow: hidden;
					display: -webkit-box;
					-webkit-box-orient:vertical;
					text-overflow:ellipsis;
					// -webkit-line-clamp:2;
				}
				p{
					span{
						color: #ff3333;
					}
				}
			}
		}
	}
	.nav-bar{
		position: fixed;
		bottom: 0;
		left: 0;
		width: 100%;
		z-index: 999;
		.pause{
			display: none;
			line-height: 24px;
			text-align: center;
			font-size: 13px;
			background: rgba(0,0,0,0.8);
			color: #fff;
			padding: 5px 0;
		}
		.nav-con{
			height: 80px/$x;
			border: 1px solid #e3e3e3;
			background: #f7f7f8;
			/*display: box;*/
			/*display: -webkit-box;*/
			display: flex;
			color:#3d4145;
			a{
				display: block;
				width: 88px/$x;
				text-align: center;
				font-size: 14px;
				padding-top: 3px;
				flex:0 1 88px/$x;
				border-right: 1px solid  #e3e3e3;
				color:#3d4145;
				&:before{
					content:'';
					display: block;
					width: 26px;
					height: 27px;
					background-position: 0 -29px;
					margin: 0 auto;
				}
			}
			.call:before{
				background-position: 0 -66px;
			}
			.collect:before{
				background-position: 0 -100px;
			}
			.collected:before{
				background-position: -53px -100px;
			}
			.buy-unable{
				flex:1 1 auto;
				line-height: 48px;
				background: gray;
				color: #fff;
				&:before{
					display: none;					
				}
			}
			.buy-now{
				flex:1 1 auto;
				line-height: 48px;
				background: #ff3333;
				color: #fff;
				&:before{
					display: none;					
				}
			}
		}
	}
}
#snapshotDetail .list-function>a.pro:after, #snapshotDetail .list-function>a.date:after {
	display: none;
}
#snapshotDetail .info h1 {
	border-bottom: 1px solid  #e3e3e3;
}
.bar-nav{
	border-bottom: 1px solid  #e3e3e3;
	.icon-left{
		color: #0cb9f3;
	}
	h1{
		font-size: 16px;
	}
}
