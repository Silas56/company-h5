					var bar = $(".slide-bar .bar-con");
					bar.find("a").removeClass("show").addClass("hide");
					$(".cur").removeClass("cur");
					var curDiv = bar.find("#nav"+idTemp);
					curDiv.addClass("cur").removeClass("hide").addClass("show");
