.bar{padding-left:36px;padding-right:68px}.bar .icon{position:absolute;left:6px}.btnsearch{padding:0 auto;border:1px solid #e3e3e3;border-radius:4px;height:1.6rem;line-height:1.6rem;margin-top:0.3rem;background-color:#f8f8f8;position:absolute;top:0;right:8px}.button{display:inline;background-color:white;border-radius:0}.content-block{margin:0}.list-block{margin:0}.list-block .item-media+.item-inner{margin:0}.center{text-align:center;background-color:white;padding:0.4rem}.category{width:100%;line-height:1rem;background:transparent;border:none;-webkit-appearance:none;-moz-appearance:none;appearance:none}.relative{display:none}.content-block a{display:block;float:left;margin:0.3rem}.content{background-color:white}.his{padding-top:1rem;clear:both}.clearHis a{border-radius:0;padding:0.3rem 0.5rem}.content-block a{border-radius:0;color:#000;border:1px solid gray}.cateValue{padding-left:10px;border-style:none;width:65%;font-size:0.8rem;color:gray}.clearCateValue{position:absolute;top:50%;right:6px;-webkit-transform:translateY(-50%);transform:translateY(-50%);display:inline-block;width:1rem;height:1rem;background:url(../../images/search/clear.png) no-repeat center;background-size:cover;margin-left:-.5rem}header .title{position:relative;width:100%;height:1.6rem;margin:0.3rem 0 0 0;padding-left:6px;background-color:#fff;border:1px solid #e3e3e3;border-radius:4px;line-height:1.6rem;text-align:left}header .select-style{display:inline-block;width:56px;height:100%;background:url(../../images/search/select_arrow.png) no-repeat right center;background-size:10px}
