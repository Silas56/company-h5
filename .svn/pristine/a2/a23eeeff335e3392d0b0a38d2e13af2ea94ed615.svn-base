#page_myCollection{
	.modal-button{color:#333333;}
	.tab{margin: 0;}
	.content{    
		// overflow: hidden;
		background: #fff;
		.content-block{
			margin:10px 0 0 0;
			padding: 0 10px;
		}
		.product_list{
			/*height: 85px;*/
			position: relative;
			overflow: hidden;
			.product_list_l{ 
				width: 100px; 
				float: left;
				position: absolute;
				top: 0;
				left: 0;
				img{
					width: 100%;
					height: 5rem;
				}
				.under_shelf{
					width: 100%;
					height: 25px; 
					background-color: #666666;
					color: #FFF;
					position: absolute; 
					bottom: 0;
					font-size: 14px; 
					text-align: center;
				}
			}
			.product_list_r{
				width: 100%; 
				float:left;
				position: relative;
				padding-left: 110px;
				height: 5rem;
				.product_list_r_title{
					width: 100%;    
					word-break: break-all;
					font-size: 0.7rem;    
					margin-bottom: 10px;
					display: -webkit-box;
					-webkit-box-orient:vertical;
					-webkit-line-clamp:2;
					overflow: hidden;
					text-overflow: ellipsis;
				}
				.product_list_r_money{color: #ff3333;font-size: 0.7rem;}
				.product_list_r_menu{
					height: 29px;
					width: 46px; 
					position: absolute; 
					bottom: -6px;  
					right: 0; 
					border-radius: 17px;
					.menu_circular{ 
						height: 4px;
						width: 4px;
						background-color: #8e8e8e;
						border-radius: 4px; 
						position: absolute;
						top: 11px;
					}
					.left{left: 29px}
					.mid{ left: 35px}
					.right{left: 41px}
				}
				.menu_click{ z-index: 1000;display: none; position: absolute; width: 100px;right: 0;top: 85px;text-align: center;
						.menu_click_1{background-image: url(../../images/home/u88.png);z-index: 20;width: 30px; height: 13px; position: absolute; top: -12px;right: 13px;	}
						.menu_click_2{border: 1px solid #e3e3e3;background-color: #FFF;  border-bottom: 0;}
						.menu_click_3{ border: 1px solid #e3e3e3;background-color: #FFF; border-bottom: 0;}
						.menu_click_4{ border: 1px solid #e3e3e3;background-color: #FFF;}
					}
			}
		}
	}

	#tab1-2{
		.content-block{
			background-color: #FFF;padding-top: 10px; padding-bottom: 10px;border-bottom: 1px solid #e3e3e3;margin: 0;
			.product_list{margin-top: 0px;height: 5rem;
				.product_list_r{
					.product_list_r_star{ font-size: 14px;
						.score{  vertical-align: middle;display: inline-block;float: left;width: 108px; height: 18px;background: url(../../images/home/icon-star.png) repeat-x 0 -18px; background-size: auto 36px;position: relative;
							em{    display: block; position: absolute;top: 0; left: 0; width: 100%; height: 18px; background: url(../../images/home/icon-star.png) repeat-x 0 1px;background-size: auto 36px;}
						}
					}
					.menu_click{ z-index: 1000;display: none;  position: absolute; width: 100px;right: 0;bottom: -66px;text-align: center;
						.menu_click_1{background-image: url(../../images/home/u88.png);width: 30px;position: absolute; top: -12px;right: 13px;}
						.menu_click_2{    border: 1px solid #e3e3e3;background-color: #FFF;    border-bottom: 0;}
						.menu_click_3{ border: 1px solid #e3e3e3;background-color: #FFF;}
					}
				}
			}
		}
	}




	.buttons-row{
		margin-top: 1px; margin: 1px auto;width: 40%;
		.button{color:#333333;}
		.button:first-child{border: 0;}
		.tab-link{
			width: 50%;background-color: #FFF;border: 0;
		}
	}

	.myCollection_shade{    height: 100%;width: 100%; position: absolute; z-index: 1000; display: none;opacity: 0.6;filter: alpha(opacity=60);
		    background-color: #333333;}
	.myCollection_share{height: 7rem;width: 100%;position: absolute;bottom:  -218px;left: 0;background-color: #eee;z-index: 1001;
		.share_top{padding: 10px 0;overflow-y: auto;
			.share_con{float: left;width: 33.33%; text-align: center;font-size: 14px;
				img{}
				p{margin: 0; color: #8e8e8e;}
			}
		}
		.share_bottom{background-color: #FFF;height: 42px; border-top: 1px solid #e3e3e3;text-align: center;line-height: 42px;font-size: 16px; color: #333333;}
	}


	.menu_click_shade{    
		height: 100%;
		width: 100%;
		position: absolute; 
		z-index: 1000; 
		display: none;
		opacity: 0.6;
		filter: alpha(opacity=60);
		background-color: #333333;
	}
	.menu_click{ 
		height:148px;
		z-index: 1001;
	    width: 100%;
	    position: absolute;
	    left: 0;
	    z-index: 1001;
	    bottom: -148px;
	    text-align: center;
	    p{
	    	margin: 0; 
	    	color: #333333;
			font-size: 12px;
	    }
	    .menu_click_shade_con{
	    	bottom: 0;
	    	opacity: 0.4;
			filter: alpha(opacity=40);
			background-color: #FFF;
			position: absolute;
			height: 100%;
			width: 100%;
			z-index: -1;
	    }
		.menu_click_con{
			padding: 15px 0;
	    	overflow-y: auto;
			background-color: #eee;
			.menu_click_2{
				float: left;
	    		width: 33%;
			}
			.menu_click_3{
				float: left;
	    		width: 33%;
			}
			.menu_click_4{
				float: left;
	    		width: 33%;
			}
		}
		.menu_click_bottom{
			height: 42px; 
			border-top: 1px solid #e3e3e3;
			line-height: 42px;
			font-size: 16px; 
			color: #333333;
			background-color: #FFF;
		}
	}


	#shop_share{
		width: 50%;
		float: left;
	}
	#shop_Cancel_collection{
		width: 50%;
		float: left;
	}	
}
